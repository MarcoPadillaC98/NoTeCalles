<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.NoTeCalles" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="Theme.NoTeCalles" parent="Base.Theme.NoTeCalles" >
        <item name="android:statusBarColor" tools:targetApi="1">@color/blueGray</item>
        <item name="android:navigationBarColor">@color/blueGray</item>

    </style>

    <style name="Theme.NoTeCalles.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

    </style>

    <style name="Theme.NoTeCalles.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" >
        <item name="android:statusBarColor" tools:targetApi="1">@color/black</item>
        <item name="android:navigationBarColor">@color/black</item>
    </style>



    <style name="Theme.NoTeCalles.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>